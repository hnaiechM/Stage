<div class="author">
  <h1 class="author__heading">Search results</h1>
</div>
<section class="articlrs">
  <ul class="article-ul">
    <!-- boucle for -->
   <% data.forEach(post => { %>
    <li>
      <a href="/post/<%= post._id %>">
        <span><%= post.title %></span>
        <span class="article-list__date"><%= post.createdAt %></span>
      </a>
    </li>
    <% }) %>
  </ul>
</section>